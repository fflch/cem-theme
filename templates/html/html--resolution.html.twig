{#
/**
 * @file
 * Default theme implementation for the basic structure of a single Drupal page.
 *
 * Variables:
 * - logged_in: A flag indicating if user is logged in.
 * - root_path: The root path of the current page (e.g., node, admin, user).
 * - node_type: The content type for the current node, if the page is a node.
 * - head_title: List of text elements that make up the head_title variable.
 *   May contain one or more of the following:
 *   - title: The title of the page.
 *   - name: The name of the site.
 *   - slogan: The slogan of the site.
 * - page_top: Initial rendered markup. This should be printed before 'page'.
 * - page: The rendered page markup.
 * - page_bottom: Closing rendered markup. This variable should be printed after
 *   'page'.
 * - db_offline: A flag indicating if the database is offline.
 * - placeholder_token: The token for generating head, css, js and js-bottom
 *   placeholders.
 *
 * @see template_preprocess_html()
 *
 * @ingroup themeable
 */
#}
<!DOCTYPE html>

<html{{ html_attributes }}>
<head>
    <head-placeholder token="{{ placeholder_token }}">
        <title>{{ head_title|safe_join(' | ') }}</title>
        <css-placeholder token="{{ placeholder_token }}">
            <js-placeholder token="{{ placeholder_token }}">
                </head>
<body{{ attributes }}>
<header class="header">
    <div class="header-top-links">
        <div class="inner wrapper">
            <div class="social">
                <ul>
                    <li class="icon-facebook">
                        <a href=" https://www.facebook.com/CemCepid" target="_blank"><span>facebook</span></a>
                    </li>
                    <li class="icon-twitter">
                        <a href="https://twitter.com/CEM_CMS" target="_blank"><span>twitter</span></a>
                    </li>
                    <li class="icon-instagram">
                      <a href="https://www.instagram.com/centrodametropole/" target="_blank"><span>instagram</span></a>
                    </li>
                </ul>
            </div>
            <div class="contact-translate">
                <ul>
                    <li class="header-top-btn-contact"><a href="/fale-conosco"><span> {% trans %}Fale Conosco e Imprensa{% endtrans %}</span></a></li>
                    <li class="header-top-btn-translate">{{ drupal_entity('block', 'seletordeidioma') }}</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="header-top">
        <div class="inner wrapper">
            <a href="/">
                <h1 class="logo">
                    <span class="title">{% trans %}CEM{% endtrans %}</span>
                    <span class="sub-title">{% trans %}Centro de Estudos da Metrópole{% endtrans %}</span>
                </h1>
            </a>
            <div class="header-search">
                {{ drupal_entity('block','formularioexpostobuscabusca_geral') }}
            </div>
        </div>
    </div>
</header>
{#
  Keyboard navigation/accessibility link to main content section in
  page.html.twig.
#}
<a href="#main-content" class="visually-hidden focusable">
    {{ 'Skip to main content'|t }}
</a>
{{ page_top }}
{{ page }}
{{ page_bottom }}
<main class="main">

</main>
<section class="footer-navigation">
    <div class="inner wrapper">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-8">
                    <div class="row footer-nav-infos">
                        <div class="col-md-5">
                            {{ drupal_entity('block', 'suporte') }}
                        </div>
                        <div class="col-md-3">
                            <div class="title">{% trans %}Menu{% endtrans %}</div>
                            <nav class="footer-nav">
                                {{ drupal_menu('footer') }}
                            </nav>
                        </div>
                        <div class="col-md-3">
                            <div class="title">{% trans %}Páginas{% endtrans %}</div>
                            <nav class="footer-nav">
                                {{ drupal_menu('paginas') }}
                            </nav>
                        </div>
                    </div>
                </div>
                <div class="col-md-4">
                    {{ drupal_entity('block','footerlicense') }}
                </div>
            </div>
        </div>
    </div>
</section>
<section class="footer-copy">
    <div class="inner wrapper">
        {% trans %}
        <p>©2021 CEM - CENTRO DE ESTUDOS DA METRÓPOLE</p>
        {% endtrans %}
    </div>
</section>
<js-bottom-placeholder token="{{ placeholder_token }}">
</body>
</html>
